{
    "name": "magento/installer",
    "description": "Magento application installer.",
    "keywords": ["magento","magento installer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Iftakharul Alam",
            "email": "info@shiftenter.dev"
        }
    ],
    "require": {
        "php": "^7.2.9",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "symfony/console": "^4.0|^5.0",
        "symfony/filesystem": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "bin": [
        "bin/magento"
    ],
    "autoload": {
        "psr-4": {
            "Magento\\Installer\\Console\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
